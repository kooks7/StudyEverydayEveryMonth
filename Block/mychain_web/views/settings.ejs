<% layout('layout/layout')%>
<div class="container">
  <h1>Create Transactoin</h1>
 
  <form action="/blockchain/settings" method="post">
     <div class="form-group">
        <label for="difficulty">블록 난이도 설정 (Difficulty)</label>
        <input type="text" class="form-control" id="difficulty"
            name="difficulty" value="">
     </div>
     <div class="form-group">
        <label for="reward">블록체인 보상 설정 (Reward)</label>
        <input type="text" class="form-control" id="reward"
            name="reward" value="">
     </div>
     <button type="submit" class="btn btn-primary">전송</button>
  </form>
</div>
